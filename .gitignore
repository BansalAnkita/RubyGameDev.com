# Ignore Mac temp files
.DS_Store

# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore development environment variables file
.env

# ignore private provisioning recipes (for now)
/config/sunzi
